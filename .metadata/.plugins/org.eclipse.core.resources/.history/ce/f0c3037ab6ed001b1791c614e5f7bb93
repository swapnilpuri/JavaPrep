package com.iks.algo;

public class SelectionSort {

	public static void main(String[] args) {
		int a[] = {34,23,64,11,84,78};
		int b[] = new int[a.length];
		min = a[0];
		int j = 0;
		for (int j=0; j < a.length; j++) {
			for (int i=0; i < a.length; i++) {
				if(min > a[i]) {
					min = a[i]
				}
			}
			b[j] = min;
		}

		
		
		
		

	}

}
